        "from PyPDF2 import PdfWriter\n",
        "import os"
      ],
      "metadata": {
        "id": "hDnupPqaGJLG"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "merger=PdfWriter()\n",
        "files=[file for file in os.listdir() if file.endswith(\".pdf\")]\n",
        "for pdf in files:\n",
        "  merger.append(pdf)"
      ],
      "metadata": {
        "id": "REWFtyhHGXXS"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "merger.write(\"merged-pdf.pdf\")\n",
        "merger.close()"
      ],
